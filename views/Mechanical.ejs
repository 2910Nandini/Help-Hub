<!DOCTYPE html>
<html lang="en" dir="ltr">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Responsive Profile Cards</title>
  <style>
    @import url('https://fonts.googleapis.com/css2?family=Poppins:wght@200;300;400;500;600;700&display=swap');

    * {
      margin: 0px;
      padding: 0px;
      box-sizing: border-box;
      font-family: 'Poppins', sans-serif;
    }

    ::selection {
      background: #d1e3ff;
      color: #fff;
    }
    .Main-div
    {
      display: flex;
      height: 60%;
      white-space: nowrap;
      overflow-y: hidden;
      overflow-y: auto;
      width: 100%;
    }

    html,
    body {
      display: grid;
      height: 100%;
      place-items: center;
      background: #d1e3ff;
    }

    .container {
      max-width: 1000px;
      display: flex;
      flex-direction: row;
      justify-content: start;
      flex-wrap: wrap;
      padding: 20px;
    }

    .container .box {
      width: calc(33% - 10px);
      background: #fff;
      display: inline-block;
      flex-direction: column;
      justify-content: center;
      align-items: center;
      padding: 20px 30px;
      border-radius: 5px;
      width: 100% !important;
    }

    .box .quote i {
      margin-top: 10px;
      font-size: 45px;
      color: #17c0eb
    }

    .container .box .image {
      margin: 10px 0;
      height: 150px;
      width: 150px;
      background: #8e44ad;
      padding: 3px;
      border-radius: 50%;
    }

    .box .image img {
      height: 100%;
      width: 100%;
      border-radius: 50%;
      object-fit: cover;
      border: 2px solid #fff;
    }

    .box p {
      text-align: justify;
      margin-top: 8px;
      font-size: 16px;
      font-weight: 400;
    }

    .box .name_job {
      margin: 10px 0 3px 0;
      color: #8e44ad;
      font-size: 18px;
      font-weight: 600;
    }

    .rating i {
      font-size: 18px;
      color: #8e44ad;
      margin-bottom: 5px;
    }

    .btns {
      margin-top: 20px;
      margin-bottom: 5px;
      display: flex;
      justify-content: space-between;
      width: 100%;
    }

    .btns button {
      background: #0355cc;
      width: 100%;
      padding: 9px 0px;
      outline: none;
      border: 2px solid rgb(40, 44, 45);
      border-radius: 5px;
      cursor: pointer;
      font-size: 18px;
      font-weight: 400;
      color: #8e44ad;
      transition: all 0.3s linear;
    }

    .btns button:first-child {
      background: none;
      margin-right: 5px;
    }

    .btns button:last-child {
      color: #fff;
      margin-left: 5px;
    }

    .btns button:first-child:hover {
      background: #8e44ad;
      color: #fff;
    }

    .btns button:hover {
      color: #d1e3ff;
    }

    @media (max-width:1045px) {
      .container .box {
        width: calc(50% - 10px);
        margin-bottom: 20px;
      }
    }

    @media (max-width:710px) {
      .container .box {
        width: 100%;
      }
    }
  </style>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.2/css/all.min.css" />
</head>

<body>
    <!-- <input type="text" id="searchInput" placeholder="Search by address line..."> -->
    <div class="Main-div">
    <% for(const key in data ) { %>
  <div class="container">
    <div class="box">
      <div class="image">
        <% if (data[key].gender === 'female') { %>
            <img src="images/female_profile.png" alt="">
          <% } else { %>
            <img src="images/male_profile.png" alt="">
          <% } %>
      </div>
      <div class="name_job"><%=  data[key].fullName %></div>
      <div class="rating">
        <% if (data[key].experience> 2) { %>
          <i class="fas fa-star"></i>
          <i class="fas fa-star"></i>
          <i class="fas fa-star"></i>
        <% } else { %>
          <i class="fas fa-star"></i>
          <i class="fas fa-star"></i>
        <% } %>
        <i class="far fa-star"></i>
        <i class="far fa-star"></i>
      </div>
      <p> <%= data[key].address.addressLine %></p>
      <p> <%= data[key].serviceType %></p>
      <div class="btns">
        <button>FOR Contact  </button>
        <button><%=  data[key].phoneNumber %></button>
      </div>
    </div>
   
  </div>
  <% } %> 
</div>
</body>
<script>
     // Get input element and profile containers
  const searchInput = document.getElementById('searchInput');
  const profileContainers = document.querySelectorAll('.box');

  // Add event listener for input changes
  searchInput.addEventListener('input', function() {
    const searchText = this.value.toLowerCase(); // Get the entered search text
    profileContainers.forEach(profile => {
      const addressLine = profile.getAttribute(' addressLine').toLowerCase(); // Get profile's address line
      // Check if the address line contains the search text, if not, hide the profile
      if (addressLine.includes(searchText)) {
        profile.style.display = 'block';
      } else {
        profile.style.display = 'none';
      }
    });
  });



</script>
</html>
